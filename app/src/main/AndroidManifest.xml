<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uin.millatip.shrimpy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.management.detail_water.DetailWaterActivity"></activity>
        <activity android:name=".ui.management.water_quality.CreateNoteActivity" />
        <activity
            android:name=".ui.calculation.activities.FcrActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ui.calculation.activities.PpmActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ui.calculation.activities.MakananActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ui.calculation.activities.PemupukanActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ui.calculation.activities.PenebaranActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ui.calculation.activities.PengapuranActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
